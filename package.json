    "prism-theme-vars": "^0.2.2",
    "prismjs": "^1.24.1",
    "style-value-types": "^4.1.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "unbuild": "^0.4.2",
